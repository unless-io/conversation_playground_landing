<!doctype html>
<html>
  <head>
  <script id="Cookiebot" src="https://consent.cookiebot.com/uc.js" data-cbid="40556afb-5bfd-4bd1-9ac2-3ed7da419932" data-blockingmode="auto" type="text/javascript"></script>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
  content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    <% if content_for?(:head) %>
      <%= yield_content :head %>
    <% end %>
    <%= stylesheet_link_tag "application" %>
    <%= favicon_tag 'images/favicon.png' %>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-163569248-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      
      gtag('config', 'UA-163569248-1');
    </script>
  </head>
  <body class="<%= current_page.data.body_class %>">
    <%= partial "shared/navbar" %>
    <%= yield %>
    <%= partial "shared/footer" %>
    <%= javascript_include_tag "application" %>
    <% if content_for?(:after_js) %>
      <%= yield_content :after_js %>
    <% end %>
  </body>
</html>
